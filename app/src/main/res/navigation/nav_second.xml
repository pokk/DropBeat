<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navSecond"
    app:startDestination="@id/fragmentSecond">

    <fragment
        android:id="@+id/fragmentSecond"
        android:name="taiwan.no.one.jurassicpark.presentation.fragment.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/activity_second">
        <action
            android:id="@+id/action_fragmentSecond_to_activitySecond"
            app:destination="@id/activitySecond">
            <argument
                android:name="id"
                android:defaultValue="0"
                app:argType="integer"/>
        </action>
        <deepLink
            android:id="@+id/deepLink"
            app:uri="main.park.com/{id}"/>
    </fragment>
    <activity
        android:id="@+id/activitySecond"
        android:name="taiwan.no.one.jurassicpark.presentation.activity.SecondActivity"
        android:label="Second"
        tools:layout="@layout/activity_second">
        <argument
            android:name="id"
            android:defaultValue="0"
            app:argType="integer"/>
        <deepLink
            android:id="@+id/deepLink"
            app:uri="second.park.com"/>
    </activity>
</navigation>
